{
  "expo": {
    "name": "esmlapp",
    "slug": "esmlapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "esmlapp",
    "owner": "eyasahbeni",
    "android": {
      "package": "com.eyasahbeni.esmlapp",
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      }
    },
    "ios": {
      "bundleIdentifier": "com.eyasahbeni.esmlapp"
    },
    "userInterfaceStyle": "automatic",
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "ccd0913d-1426-45c9-b156-95f40f69ede5"
      }
    }
  }
}
